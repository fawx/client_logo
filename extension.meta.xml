<?xml version="1.0" encoding="UTF-8" ?>
<extension id="client_logo" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">

    <name>Client Logo</name>

    <description>This extension gives you the option to replace Symphony's default admin header with an image.  It was born out of a desire to allow clients to feel like their CMS is their own.</description>

    <types>
        <type>Interface</type>
    </types>

    <dependencies>
        <dependency>jit_image_manipulation</dependency>
    </dependencies>

    <repo type="github">https://github.com/fawx/client_logo</repo>

    <url type="discuss">http://symphony-cms.com/discuss/thread/58934/</url>
    <url type="issues">https://github.com/fawx/client_logo/issues</url>

    <authors>
        <author>
            <name github="fawx" symphony="fawx">Adam Fox</name>
        </author>
    </authors>

    <releases>
    	<release version="1.2" date="2012-07-13" min="2.3">
    		- Fix code for compatibility with Symphony 2.3
    	</release>
        <release version="1.1.1" date="2012-05-03" min="2.2" max="2.2.5" />
    </releases>
</extension>
